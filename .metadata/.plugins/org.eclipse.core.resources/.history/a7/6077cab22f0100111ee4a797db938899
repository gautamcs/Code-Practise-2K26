package com.gautam.model;

public class Student {
	
	private String name; 
	private int roll; 
	private String course;
	private Address address; 
	
	public Student(String name, int roll, String course, Address address) {
		this.name = name; 
		this.roll = roll; 
		this.course  = course; 
		this.address = address; 
	}
	
	
}
