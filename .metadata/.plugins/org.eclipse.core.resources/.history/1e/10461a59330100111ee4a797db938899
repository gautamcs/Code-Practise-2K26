package com.gautam.main;

import com.gautam.model.Address;
import com.gautam.model.Student;

public class ImmutableClassTest {
	public static void main(String[] args) {
		
		Address address = new Address("Hyderabad", "Telangana", 500008, "India");
		Student student = new Student("Gautam", 56, "MCA", address);
		
		System.out.println(" Original Data..... ");
		System.out.println(" Address: "+address.toString());
		System.out.println(" Student: "+student.toString());		
		
		System.out.println(" Modification done in Address Object: ");
		Address address2 = new Address("Jamui", "Bihar", 811312, "India");
		Student student2 = new Student("Gautam", 56, "MCA", address2);
		
		System.out.println(" Address: "+address2.toString());
		System.out.println(" Student: "+student2.toString());
		
	
		address.setPinCode(803107);
		System.out.println(" Student: "+student.toString());	
		
	}
}
